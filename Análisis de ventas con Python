    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Simular datos de ventas de una tienda\n",
    "np.random.seed(42)\n",
    "fechas = pd.date_range('2025-01-01', '2025-12-31', freq = 'D')\n",
    "ventas = np.random.randint(5000, 25000, size=len(fechas))\n",
    "categorias = np.random.choice(['Bebidas', 'Snacks', 'Combustible', 'Otros'], size=len(fechas))\n",
    "\n",
    "df = pd.DataFrame({\n",
    "    'fecha': fechas,\n",
    "    'ventas': ventas,\n",
    "    'categoria': categorias\n",
    "})\n",
    "\n",
    "df.to_csv('ventas_tienda.csv', index=False)"
   ]
  },
  {
