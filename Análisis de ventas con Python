   "source": [
    "# Leer datos\n",
    "df = pd.read_csv('ventas_tienda.csv')\n",
    "df['fecha'] = pd.to_datetime(df['fecha'])\n",
    "df['mes'] = df['fecha'].dt.month\n",
    "df['mes_nombre'] = df['fecha'].dt.strftime('%b')  # Nombre completo del mes\n",
    "print(df)"
   ]
  },
  {
   "cell_type": "code",
