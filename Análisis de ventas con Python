   ],
   "source": [
    "\n",
    "# Análisis 1: ventas totales por mes\n",
    "ventas_mes = df.groupby(['mes', 'mes_nombre'])['ventas'].sum().reset_index()\n",
    "ventas_mes = ventas_mes.sort_values('mes')  # Ordenar por número de mes\n",
    "print(\"Ventas por mes:\")\n",
    "print(ventas_mes)\n",
    "\n",
    "# Análisis 2: Ventas promedio por categoria\n",
    "ventas_categoria = df.groupby('categoria')['ventas'].mean()\n",
    "print(\"\\nVentas promedio por categoria:\")\n",
    "print(ventas_categoria)\n",
    "\n",
    "# Visualización\n",
    "plt.figure(figsize=(12,6))\n",
    "plt.bar(ventas_mes['mes_nombre'], ventas_mes['ventas'], color='steelblue', edgecolor='black')\n",
    "plt.title('Ventas Totales por Mes - 2025', fontsize=16, fontweight='bold')\n",
    "plt.xlabel('Mes', fontsize=12)\n",
    "plt.ylabel('Ventas ($)', fontsize=12)\n",
    "plt.grid(axis='y', alpha=0.3)  # Agregar grid para mejor visualización\n",
    "plt.tight_layout()  # Ajustar layout para que no se corten las etiquetas\n",
    "plt.savefig('ventas_mensuales.png', dpi=300, bbox_inches='tight')\n",
    "plt.show()\n",
    "plt.close()\n",
    "\n",
    "print(\"\\n✅ Análisis completado!\")"
   ]
